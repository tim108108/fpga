#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/va_math.vpi";
S_0x559a7c2dc0 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x559a7c2f90_0 .var "clk", 0 0;
v0x559a80d100_0 .var "cnt", 5 0;
v0x559a80d1e0_0 .var "rstn", 0 0;
E_0x559a7c39d0 .event edge, v0x559a80d100_0;
E_0x559a7c33c0/0 .event negedge, v0x559a80d1e0_0;
E_0x559a7c33c0/1 .event posedge, v0x559a7c2f90_0;
E_0x559a7c33c0 .event/or E_0x559a7c33c0/0, E_0x559a7c33c0/1;
    .scope S_0x559a7c2dc0;
T_0 ;
    %delay 5, 0;
    %load/vec4 v0x559a7c2f90_0;
    %inv;
    %store/vec4 v0x559a7c2f90_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x559a7c2dc0;
T_1 ;
    %wait E_0x559a7c33c0;
    %load/vec4 v0x559a80d1e0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x559a80d100_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x559a80d100_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x559a80d100_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x559a7c2dc0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a7c2f90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559a80d1e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559a80d1e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559a80d1e0_0, 0, 1;
    %vpi_call 2 23 "$display", "Hello ICARUS verilog!" {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x559a7c2dc0;
T_3 ;
    %wait E_0x559a7c39d0;
    %vpi_call 2 29 "$display", "%0t, cnt=%d", $time, v0x559a80d100_0 {0 0 0};
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x559a7c2dc0;
T_4 ;
    %vpi_call 2 34 "$dumpfile", "tb.vcd" {0 0 0};
    %vpi_call 2 35 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559a7c2dc0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "tb.v";
